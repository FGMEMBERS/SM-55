<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>inttour</object-name>
    <object-name>inttourvitre</object-name>

    <!-- Objets transparents -->
    <object-name>intverriere</object-name>
  </animation>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>intverriere</object-name>
  </effect>
  
  <!-- Pilote -->
  <model>
    <path>Aircraft/SM-55/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -4.788 </x-m>
      <y-m>  0.280 </y-m>
      <z-m> -0.510 </z-m>
    </offsets>
  </model>

  <!-- Tableau de bord -->
  <model>
    <path>Aircraft/SM-55/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -5.075 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.264 </z-m>
    </offsets>
  </model>

</PropertyList>
